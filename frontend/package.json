{
	"name": "frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev:deploy": "vite",
		"dev": "cd .. && docker-compose -f docker-compose-dev.yml up -d && cd frontend && vite serve --mode development",
		"down": "cd .. && docker-compose -f docker-compose-dev.yml down",
		"build": "tsc && vite build --mode production",
		"preview": "vite preview",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"cy:open-e2e": "cypress open --e2e --browser chrome",
		"cy:open-unit": "cypress open --component --browser chrome",
		"cy:run-e2e": "cypress run --e2e",
		"cy:run-unit": "cypress run --component",
		"cy:e2e": "start-server-and-test start http-get://localhost:3000 cy:open-e2e",
		"style:prettier": "prettier --check .",
		"format": "prettier --write .",
		"style:all": "tsc && yarn lint && yarn style:prettier",
		"prepare": "cd .. && husky install frontend/.husky"
	},
	"dependencies": {
		"@ant-design/icons": "^5.0.1",
		"@reduxjs/toolkit": "^1.9.3",
		"antd": "^5.3.2",
		"axios": "^1.3.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.0.5",
		"react-router-dom": "^6.9.0",
		"vite-plugin-svgr": "^2.4.0"
	},
	"devDependencies": {
		"@cypress/react18": "^2.0.0",
		"@types/node": "^18.15.3",
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.10",
		"@typescript-eslint/eslint-plugin": "5.13.0",
		"@typescript-eslint/parser": "5.0.0",
		"@vitejs/plugin-react": "^3.1.0",
		"autoprefixer": "^10.4.14",
		"cypress": "^12.8.0",
		"eslint": "8.24.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-typescript": "17.0.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-import-resolver-typescript": "3.5.1",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-jsx-a11y": "6.6.1",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-react": "7.31.8",
		"eslint-plugin-react-hooks": "4.6.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.2.0",
		"postcss": "^8.4.21",
		"prettier": "2.7.1",
		"start-server-and-test": "^2.0.0",
		"tailwindcss": "^3.2.7",
		"typescript": "^4.9.3",
		"vite": "^4.1.0"
	},
	"lint-staged": {
		"**/*.{ts,tsx}": [
			"yarn style:all"
		]
	}
}